{
  "name": "@artiffine/hardhat",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "chain": "npx hardhat node",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "format": "prettier --write \"*/**/*.{js,ts,tsx,css,md,json,sol}\"",
    "deploy:localhost": "npx hardhat run scripts/deploy.ts --network localhost",
    "deploy:rinkeby": "npx hardhat run scripts/deploy.ts --network rinkeby",
    "deploy:mainnet": "npx hardhat run scripts/deploy.ts --network mainnet",
    "read:localhost": "npx hardhat run scripts/read.ts --network localhost",
    "read:rinkeby": "npx hardhat run scripts/read.ts --network rinkeby",
    "read:mainnet": "npx hardhat run scripts/read.ts --network mainnet",
    "write:localhost": "npx hardhat run scripts/write.ts --network localhost",
    "write:rinkeby": "npx hardhat run scripts/write.ts --network rinkeby",
    "write:mainnet": "npx hardhat run scripts/write.ts --network mainnet",
    "flatten": "npx hardhat flatten contracts/NFTContract.sol > output.sol",
    "etherscan:verify:rinkeby": "npx hardhat run scripts/etherscanVerify.ts --network rinkeby",
    "etherscan:verify:mainnet": "npx hardhat run scripts/etherscanVerify.ts --network mainnet"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.6.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.29",
    "@types/prompt-sync": "^4.1.1",
    "chai": "^4.3.6",
    "chalk": "^4.1.2",
    "cli-select": "^1.1.2",
    "erc721a": "^3.3.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.4",
    "figures": "^4.0.1",
    "hardhat": "^2.9.3",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "prompt-sync": "^4.2.0",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.3"
  }
}
